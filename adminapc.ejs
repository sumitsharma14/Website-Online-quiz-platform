<html>
    <body>
            <center><h1>APC QUESTIONS AND ANSWERS</h1>
       <table bgcolor="#00FF00" width="100%" >
           <tr>
               <th>QNo</th>
               <th>Qname</th>
               <th>Answer</th>
           </tr>
           <%users.forEach(function(user){%>
            <tr>
                <td><%=user.Qno%></td>
                <td><%=user.Qname%></td>
                <td><%=user.ans%></td>
            </tr>
            <%});%>
       </table>
    </center>
    </body>
</html>