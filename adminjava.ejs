<html>
    <body>
            <center><h1>JAVA QUESTIONS AND ANSWERS</h1>
       <table bgcolor="#00FF00" width="100%" >
           <tr>
               <th>QNo</th>
               <th>Qname</th>
               <th>Answer</th>
           </tr>
           <%jusers.forEach(function(juser){%>
            <tr>
                <td><%=juser.Qno%></td>
                <td><%=juser.Qname%></td>
                <td><%=juser.ans%></td>
            </tr>
            <%});%>
       </table>
    </center>
    </body>
</html>